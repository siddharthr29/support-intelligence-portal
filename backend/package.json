{
  "name": "freshdesk-weekly-support-intelligence-backend",
  "version": "1.0.0",
  "description": "Read-Only Analytics and Insight Platform for Freshdesk Support Data",
  "main": "dist/server.js",
  "scripts": {
    "build": "prisma generate && tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate",
    "set-roles": "ts-node src/scripts/set-user-roles.ts",
    "create-user": "ts-node src/scripts/create-firebase-user.ts",
    "update-password": "ts-node src/scripts/update-user-password.ts",
    "update-email": "ts-node src/scripts/update-user-email.ts",
    "list-users": "ts-node src/scripts/list-users.ts",
    "revoke-tokens": "ts-node src/scripts/revoke-user-tokens.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "freshdesk",
    "analytics",
    "read-only",
    "support-intelligence"
  ],
  "license": "PRIVATE",
  "dependencies": {
    "@fastify/compress": "^6.5.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.1.1",
    "@prisma/client": "^5.7.0",
    "@sparticuz/chromium": "^143.0.0",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.0",
    "firebase": "^10.7.0",
    "firebase-admin": "^13.6.0",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^3.0.3",
    "papaparse": "^5.5.3",
    "pino": "^8.17.0",
    "prisma": "^5.7.0",
    "puppeteer": "^24.34.0",
    "puppeteer-core": "^24.34.0",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/papaparse": "^5.5.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "pino-pretty": "^13.1.3",
    "ts-node-dev": "^2.0.0"
  }
}
